{
  "expo": {
    "name": "TaxiCalc",
    "slug": "taxi-calc",
    "version": "1.1.0a",
    "scheme": "taxicalc",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./assets/taxi-app-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/taxi-app-splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#dac66b"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.koleradev.taxicalc",
      "adaptiveIcon": {
        "foregroundImage": "./assets/taxi-app-icon.png"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme":"taxicalc",
            "host": "*"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/taxi-app-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": "http://localhost",
        "unstable_settings": {
          "initialRouteName": "(modals)/[id]"
        }
      },
      "eas": {
        "projectId": "4c8884ea-8340-4569-b67e-e5bac66f9bc5"
      }
    }
  }
}
